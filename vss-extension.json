{
    "manifestVersion": 1,
    "id": "julioarruda-widgets",
    "version": "1.0.14",
    "name": "Julio Arruda Widgets",
    "description": "Helpful widgets from Julio Arruda YouTube Channel",
    "publisher": "julioarrudac",
	"categories": ["Collaborate"],
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Julio Arruda",
        "Widget"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "links": {
        "repository": {
          "uri": "https://github.com/julioarruda/YoutubeWidget"
        },
        "issues": {
          "uri": "https://github.com/julioarruda/YoutubeWidget/issues"
        },
        "support": {
          "uri": "https://github.com/julioarruda/YoutubeWidget"
        }
      },
      "repository": {
        "type": "git",
        "uri": "https://github.com/julioarruda/YoutubeWidget"
      },
    "files": [
        { "path": "youtube-channel.html", "addressable": true },
        { "path": "sdk/scripts", "addressable": true },
        { "path": "img", "addressable": true },
        { "path": "scripts", "addressable": true },
        { "path": "styles", "addressable": true }
    ],
    "contributions": [
        {
            "id": "JulioArrudaYouTubeWidget",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Julio Arruda Playlist Browser",
                "description": "Browse Julio Arruda playlists on YouTube",                         
                "uri": "youtube-channel.html",
                "catalogIconUrl": "img/logo.png",
                "supportedSizes": [
                     {
                            "rowSpan": 4,
                            "columnSpan": 6
                        }
                    ],
                "supportedScopes": ["project_team"]
            }
        }
    ]
}
